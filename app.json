      "bundleIdentifier": "findyourconcierge.com",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#484646"
      },
      "package": "com.bernardin2202.Villa"
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    }
  }
}